<div class=" col-xs-{{ widget.position.column }}">
    <div class="box">

        <div class="box-header with-border">
            <h3 class="box-title">{{ widget.title }}</h3>
        </div>
        <div class="box-body widget-value" id="{{ widget.key }}">
            <iframe src="{{ widget.query|raw }}" scrolling="no" width="100%" height="450px"></iframe>
        </div>
    </div>
</div>